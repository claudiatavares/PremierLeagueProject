WHERE Team = 'Crystal Palace'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 12
WHERE Team = 'Chelsea'

SELECT *
FROM PremierLeagueHeights2223
WHERE Team LIKE '%Wolverhampton%' -- To confirm the full name of the Wolverhampton team

UPDATE PremierLeagueHeights2223
SET Final_Classification = 13
WHERE Team = 'Wolverhampton Wanderers'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 14
WHERE Team = 'West Ham United'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 15
WHERE Team = 'Bournemouth'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 16
WHERE Team = 'Nottingham Forest'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 17
WHERE Team = 'Everton'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 18
WHERE Team = 'Leicester City'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 19
WHERE Team = 'Leeds United'

UPDATE PremierLeagueHeights2223
SET Final_Classification = 20
WHERE Team = 'Southampton'

-- Update a column name
EXEC sp_rename 'PremierLeagueHeights2223.Height', 'Height_cm', 'COLUMN';

-- Review table with new column
-- Ordered by Final_Classification in ascendent order
SELECT *
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL
ORDER BY Final_Classification

-- DESCRIPTIVE STATISTICS
-- Average height in the league
SELECT
	AVG(Height_cm) AS Avg_Height_cm_league
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL

-- Average height by team, ordered by height in descendent order
SELECT
	Team,
	AVG(Height_cm) AS Avg_Height_cm_team
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL
GROUP BY Team
ORDER BY Team

-- Average height by position, ordered by height in descendent order
SELECT
	Position,
	AVG(Height_cm) AS Avg_Height_cm_position
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL
GROUP BY Position
ORDER BY Position

-- Min and Max height in the league
SELECT
	MIN(Height_cm) AS Min_Height_cm_league,
	MAX(Height_cm) AS Max_Height_cm_league
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL

-- Min and Max height by team, ordered by height in descendent order
SELECT
	Team,
	MIN(Height_cm) AS Min_Height_cm_team,
	MAX(Height_cm) AS Max_Height_cm_team
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL
GROUP BY Team
ORDER BY Team

-- Min and Max height by position, ordered by height in descendent order
SELECT
	Position,
	MIN(Height_cm) AS Min_Height_cm_position,
	MAX(Height_cm) AS Max_Height_cm_position
FROM PremierLeagueHeights2223
WHERE Height_cm IS NOT NULL
GROUP BY Position
ORDER BY Position
